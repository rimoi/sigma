{% extends 'base_admin.html.twig' %}

{% block title %}Modifier le flux {{ flux_rss.title }}{% endblock %}

{% block body %}
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="{{ path('home') }}">Accueil</a></li>
        <li class="breadcrumb-item"><a href="{{ path('admin_dash_board_index') }}">Dashboard</a></li>
        <li class="breadcrumb-item">
            <a href="{{ path('app_back_office_flux_rss_index') }}">Flux RSS</a>
        </li>
        <li class="breadcrumb-item active">{{ block('title') }}</li>
    </ol>
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary text-center">{{ block('title') }}</h6>
        </div>
        {% include 'back_office/flux_rss/_form.html.twig' %}
        <div class="text-right">
            {{ include('back_office/flux_rss/_delete_form.html.twig') }}
        </div>
    </div>
{% endblock %}
